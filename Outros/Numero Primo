//Verifica se um número inteiro é primo
#include <stdio.h>
#include <stdlib.h>


main()
{
  
    int  i,  num, primo;
   primo = 0;
      printf("************Verifica se o número é primo*************** \n");
      printf("Digite um numero inteiro: \n ");
      scanf("%d", &num);
      printf("\n\n\n");
 
          for (i = 1; i <= num; i++)
          {
              if (num % i == 0)
              primo ++;
          }
        
    printf("\n\n\n");
    if (primo == 2)
    printf("Numero primo");
    else
    printf("Numero nao primo");
    printf("\n\n\n");
   
    system("pause");
}